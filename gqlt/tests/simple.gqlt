let 1 = 1;
assert(1 matches 1);

let dogs = query {
	animals {
		dogs {
			list {
				name
			}
		}
	}
};

let name = query {
	animals {
		dogs {
			first {
				name
			}
		}
	}
};

assert(name matches "Buddy");

let { id, name } = query ($name: String!) {
	animals {
		dogs {
			find(name: $name) {
				id
				name
			}
		}
	}
};

